    "cors": "^2.8.4",
    "dotenv": "^6.2.0",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "graphql": "^0.10.3",
    "graphql-tools": "^3.0.2",
    "jsonwebtoken": "^8.3.0",
